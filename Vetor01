package Questões_Vetores_POO;

import java.util.Scanner;
 
 
public class Vertor01 {
    
    public static void main(String[] args) {
        
        Scanner scan = new Scanner(System.in);
        int[] vetA = new int[5];
        int[] vetB = new int[5];
        
        for(int i = 0; i < vetA.length; i++){
            System.out.println("Informe o valor de posição do elemento: " + i);
            vetA[i] = scan.nextInt();
        }
        
        for(int i = 0; i < vetA.length; i++){
            vetB[i] = vetA[i];
        }
          
        
    }
}
